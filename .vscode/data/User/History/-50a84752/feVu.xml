<mvc:View controllerName="s331102.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List items="{northWind>/Products}" title="Produkte"> 
                <items>
                    <StandardListItem title="{northWind>/Name}" description="{northWind>/ID}"></StandardListItem>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
